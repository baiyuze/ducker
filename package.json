{
  "name": "ducker",
  "version": "0.0.1",
  "description": "A minimal Electron application",
  "main": "main.js",
  "scripts": {
    "bash": "chmod 777 install.sh && bash install.sh",
    "dev": "electron-forge start",
    "start": "cp ./config/env.local ./config/env.json && electron-forge start & cd ./app/web && npm start",
    "make": "electron-forge make",
    "build": "cp ./config/env.prod ./config/env.json && node build.js",
    "build:all": "electron-builder -m"
  },
  "repository": "https://github.com/electron/electron-quick-start",
  "keywords": [
    "Electron",
    "quick",
    "start",
    "tutorial",
    "demo"
  ],
  "author": "GitHub",
  "license": "MIT",
  "devDependencies": {
    "@electron-forge/cli": "^6.0.0-beta.61",
    "@electron-forge/maker-deb": "^6.0.0-beta.61",
    "@electron-forge/maker-rpm": "^6.0.0-beta.61",
    "@electron-forge/maker-squirrel": "^6.0.0-beta.61",
    "@electron-forge/maker-zip": "^6.0.0-beta.61",
    "babel-cli": "^6.6.5",
    "babel-preset-es2015": "^6.6.0",
    "electron": "^15.3.0",
    "electron-builder": "^22.14.5",
    "mocha": "^6.1.4",
    "shelljs": "^0.8.4"
  },
  "dependencies": {
    "agentkeepalive": "^4.1.4",
    "antd": "^4.17.3",
    "anyproxy": "4.1.0",
    "babel-plugin-transform-async-to-generator": "^6.8.0",
    "babel-polyfill": "^6.13.0",
    "charset": "^1.0.1",
    "colors": "^1.1.2",
    "commander": "^2.9.0",
    "cross-os-proxy": "^2.0.0",
    "debug": "^2.2.0",
    "electron-log": "^4.4.4",
    "electron-squirrel-startup": "^1.0.0",
    "express": "^4.18.1",
    "http-proxy": "^1.18.1",
    "iconv-lite": "^0.6.3",
    "ip": "^1.1.2",
    "jschardet": "^3.0.0",
    "lodash": "^4.17.14",
    "mini-proxy": "^1.0.4",
    "mkdirp": "^1.0.4",
    "node-forge": "^0.10.0",
    "node-mitmproxy": "3.1.1",
    "nopt": "3.0.x",
    "os-proxy-config": "^1.1.1",
    "program": "^1.0.0",
    "regedit": "^5.0.0",
    "sudo-prompt": "^9.2.1",
    "through2": "^2.0.5",
    "tunnel-agent": "^0.6.0",
    "underscore": "^1.13.1",
    "urllib": "^2.38.0"
  },
  "config": {
    "forge": {
      "packagerConfig": {},
      "makers": [
        {
          "name": "@electron-forge/maker-squirrel",
          "config": {
            "name": "electron_quick_start"
          }
        },
        {
          "name": "@electron-forge/maker-zip",
          "platforms": [
            "darwin"
          ]
        },
        {
          "name": "@electron-forge/maker-deb",
          "config": {}
        },
        {
          "name": "@electron-forge/maker-rpm",
          "config": {}
        }
      ]
    }
  }
}
